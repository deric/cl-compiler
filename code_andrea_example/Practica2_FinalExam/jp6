Program
  Vars
    x Int
    y Struct
      x bool
      y Int
      EndStruct
    z Array [10] Of bool
  EndVars
  Procedure P(ref a Array [10] Of bool,
              val b Struct x Bool y Int EndStruct,
              ref x Array [4] Of Int)
    Vars
      x Array [10] Of bool
      y Array [4] of Int
    EndVars
    x[3]:=45+z[y.x]
    x:=a
    x:=y
  EndProcedure 
  x:=z[3]
  z[y.y+2]:=
            z[x]+
            y.x[3].y
EndProgram
