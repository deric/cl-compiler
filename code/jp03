PROGRAM
  VARS
    x int
    y bool
    b bool
  ENDVARS
  IF NOT x THEN
    WHILE NOT 3>4 DO
      IF (---4=(y+3))=b OR b THEN
        x:=b>(3>4)
      ENDIF
    ENDWHILE
    IF NOT -3=-3 OR (b OR b AND b) THEN
      x:=3+y+x / (3 * 4---x)
    ENDIF
  ELSE
    y:=(3>x) OR (b=b) AND (y=x)
    b:=NOT NOT NOT (b OR (x>y))
    x:=6/3
  ENDIF
ENDPROGRAM