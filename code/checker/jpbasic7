PROGRAM
  VARS
    X INT
    Y INT
  ENDVARS

  PROCEDURE F2(VAL X INT, REF Y INT)
    PROCEDURE F21(VAL X INT, REF Y INT)

      PROCEDURE F211(VAL X INT, REF Y INT)
      ENDPROCEDURE

      PROCEDURE F212(VAL X INT, REF Y INT)
      ENDPROCEDURE

    ENDPROCEDURE

    PROCEDURE F22(VAL X INT, REF Y INT)

      PROCEDURE F221(VAL X INT, REF Y INT)
      ENDPROCEDURE

      PROCEDURE F222(VAL X INT, REF Y INT)
      Y := Y-1
      IF 0 < Y THEN F22(X,Y) ELSE X := 1 ENDIF
      ENDPROCEDURE

    IF 0 < X THEN F222(X-1,Y) ELSE X := 2 ENDIF
    ENDPROCEDURE

  F1(X, Y)
  ENDPROCEDURE

  PROCEDURE F1(VAL X INT, REF Y INT)
    PROCEDURE F11(VAL X INT, REF Y INT)

      PROCEDURE F111(VAL X INT, REF Y INT)
        WRITE(111)
      ENDPROCEDURE

      PROCEDURE F112(VAL X INT, REF Y INT)
        WRITE(112)
      ENDPROCEDURE
      WRITE(11)
      F12(X, Y)
    ENDPROCEDURE

    PROCEDURE F12(VAL X INT, REF Y INT)

      PROCEDURE F121(VAL X INT, REF Y INT)
        WRITE(121)
      ENDPROCEDURE

      PROCEDURE F122(VAL X INT, REF Y INT)
        WRITE(122)
      ENDPROCEDURE
      WRITE(12)
    ENDPROCEDURE
  F11(X, Y)
  ENDPROCEDURE
X := 1
Y := 2
F2(X,Y)
WRITELN(Y)
ENDPROGRAM