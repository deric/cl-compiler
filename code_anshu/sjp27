

  1: PROGRAM
  2:   VARS
  3:     X INT
  4:     Y INT
  5:   ENDVARS
  6:   FUNCTION FACT(VAL X INT) RETURN INT
  7:   VARS 
  8:     F INT
  9:   ENDVARS
 10:     IF X = 1 THEN
 11:       F := 1
 12:     ELSE
 13:       F := FACT(X-1) * X
 14:     ENDIF
 15:     RETURN F
 16:   ENDFUNCTION
 17:   X:= 1
 18:   WHILE (X < 10) DO
 19:     WRITE(" El factorial de ")
 20:     WRITE(X)
 21:     WRITE(" es: ")
 22:     WRITELN(FACT(X))
 23:     X := X+1
 24:   ENDWHILE
 25: ENDPROGRAM


