endprogram

subroutine program_FACT
  parameters
    returnvalue
    _X
    static_link
  endparameters

  variables
    _F 4
  endvariables

    load _X t0
    iload 1 t1
    equi t0 t1 t0
    fjmp t0 else_1
    aload _F t0
    iload 1 t1
    stor t1 t0
    ujmp endif_1
  etiq else_1
    aload _F t0
    pushparam 0
    load _X t1
    iload 1 t2
    subi t1 t2 t1
    pushparam t1
    load static_link t1
    pushparam t1
    call program_FACT
    killparam
    killparam
    popparam t1
    load _X t2
    muli t1 t2 t1
    stor t1 t0
  etiq endif_1
    load _F t0
    stor t0 returnvalue
    retu
endsubroutine

